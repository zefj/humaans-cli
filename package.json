{
  "type": "module",
  "bin": {
    "humaans": "./bin/run.js"
  },
  "contributors": [
    "Andrei Zvonimir CrnkoviÄ‡ @andreicek",
    "Filip Rec @zefj"
  ],
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2"
  },
  "description": "A simple CLI for humaans.io",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8.56.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.10"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/zefj/humaans-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "humaans-cli",
  "oclif": {
    "bin": "humaans",
    "dirname": "humaans",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ],
    "topicSeparator": " ",
    "topics": {
      "clock": {
        "description": "Clock in or out"
      }
    }
  },
  "repository": "zefj/humaans-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.1.5",
  "bugs": "https://github.com/zefj/humaans-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
